<!DOCTYPE html>
<html>
  <head>
    <title>Jimoto</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
　　<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
     <!--地図表示-->
    <script src="https://maps.google.com/maps/api/js?key=<%= ENV['GOOGLE_MAP_API_KEY']%>" type="text/javascript"></script>
    <script src='https://cdnjs.cloudflare.com/ajax/libs/markerclustererplus/2.1.4/markerclusterer.min.js' type='text/javascript'></script>
  </head>

  <body>
    <%= render "layouts/header" %>
    <%# breadcrumb :top, @user %>
    <%#= breadcrumbs separator: " &rsaquo; " %>
    <%= yield %>
    <%= render "layouts/footer" %>
  </body>
</html>
