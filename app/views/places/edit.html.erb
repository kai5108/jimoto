<h3>Post edit</h3>
<%= form_with model: @place, local:true do |f| %>
  <span>画像<%= f.file_field :image %></span>
  <span>観光地名<%= f.text_field :name %></span>
  <span>住所<%= f.text_field :address %></span>
  <span>説明<%= f.text_area :capitan %></span>
  <button><%= f.submit 'Edit' %></button>
  <%= link_to :back do %>
   <button>Back</button>
  <% end %>
<% end %>